DEFINE LANGUAGE Python
    /INDENT_SIZE=4

END DEFINE

;;------------------------------------------------------------------------------

DELETE PLACEHOLDER ", IDENTIFIER"
  /LANGUAGE="Python"
DEFINE PLACEHOLDER ", IDENTIFIER"
  /LANGUAGE="Python"
  /NOAUTO_SUBSTITUTE
  /DESCRIPTION=""
  /DUPLICATION=CONTEXT_DEPENDENT
  /SEPARATOR=""
  /TYPE=NONTERMINAL

  ", {identifier}"

END DEFINE

