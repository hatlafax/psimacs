    /LANGUAGE="Python"
    /NOAUTO_SUBSTITUTE
    /DESCRIPTION=""
    /DUPLICATION=CONTEXT_DEPENDENT
    /SEPARATOR=""
    /TYPE=NONTERMINAL

    "def __enter__(self)[ -> {type_hint}]:"
    "  [document_string]"
    "  {statement}..."
    "  [return_stmt]"
    ""
    "\@overload"
    "def __exit__("
    "    self,"
    "    exc_type: None,"
    "    exc_val: None,"
    "    exc_tb: None)[ -> {type_hint}]:"
    "  ..."
    ""
    "\@overload"
    "def __exit__("
    "    self,"
    "    exc_type: type[BaseException],"
    "    exc_val: BaseException,"
    "    exc_tb: TracebackType)[ -> {type_hint}]:"
    "  ..."
    ""
    "def __exit__("
    "    self,"
    "    exc_type: type[BaseException] | None,"
    "    exc_val: BaseException | None,"
    "    exc_tb: TracebackType | None)[ -> {type_hint}]:"
    "  [document_string]"
    "  {statement}..."
    "  [return_stmt]"
    ""

END DEFINE

