    "module_compilation_unit"/PLACEHOLDER
    "unittest_compilation_unit"/PLACEHOLDER
    "unittest_compilation_unit_query"/PLACEHOLDER

END DEFINE

;;------------------------------------------------------------------------------

DELETE PLACEHOLDER UNITTEST_COMPILATION_UNIT
    /LANGUAGE="Python"
DEFINE PLACEHOLDER UNITTEST_COMPILATION_UNIT
    /LANGUAGE="Python"
    /NOAUTO_SUBSTITUTE
    /DESCRIPTION=""
    /DUPLICATION=CONTEXT_DEPENDENT
    /SEPARATOR=""
    /TYPE=NONTERMINAL

    "[hashbang_line]"
    "[license]"
    """"Unit test for module {test_module_name}."

    "[text]..."
    """""
    ""
    "import unittest"
    "from {test_module_name} import {test_class_name}"
    "[import_stmt]..."
    ""
    "class {test_class_name}Test(unittest.TestCase):"
    "  """Unit test of class {test_class_name} from module {test_module_name}."

    "  [text]..."
    "  """"
    "  [classdef]..."
    "  __verbose: bool = False"
    "  [class_variables]..."
    "  [init_def]"
    ""
    "  def setUp(self) -> None:"
    "    [document_string]"
    "    if self.__verbose: print("running setUp of {test_class_name}Test ...")"
    "    [statement]..."
    ""
    "  def tearDown(self) -> None:"
    "    [document_string]"
    "    if self.__verbose: print("running tearDown of {test_class_name}Test ...")"
    "    [statement]..."
    ""
    "  [unittest_funcdef]..."
    "  [class_funcdef]..."
    ""
    "[statement]..."
    ""
    "if __name__ == '__main__':"
    "  unittest.main()"

END DEFINE

;;------------------------------------------------------------------------------

DELETE PLACEHOLDER UNITTEST_COMPILATION_UNIT_QUERY
    /LANGUAGE="Python"
DEFINE PLACEHOLDER UNITTEST_COMPILATION_UNIT_QUERY
    /LANGUAGE="Python"
    /NOAUTO_SUBSTITUTE
    /DESCRIPTION=""
    /DUPLICATION=CONTEXT_DEPENDENT
    /SEPARATOR=""
    /AFTER=psimacs/else/python/placeholder/unittest-1
    /TYPE=NONTERMINAL

    ;; In case for solely inserttion of generated text by the /AFTER function
    ;; it will not work to give an empty terminal string. This leads to reinsertion
    ;; of the placeholder and is typically not what you want to achieve.
    ;; For that reason we provide a dummy terminal character  and delete that character
    ;; in the /AFTER functino. Not perfect but should work.

    "-"

END DEFINE

