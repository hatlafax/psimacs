DELETE PLACEHOLDER SUPER_CTOR_STMT
    /LANGUAGE="Python"
DEFINE PLACEHOLDER SUPER_CTOR_STMT
    /LANGUAGE="Python"
    /NOAUTO_SUBSTITUTE
    /DESCRIPTION=""
    /DUPLICATION=VERTICAL
    /SEPARATOR=""
    /TYPE=MENU

    "super().__init__([expression]...)"
    "super().__init__([argument_list])"
    "super([expression], self).__init__([expression]...)"
    "super([expression], self).__init__([argument_list])"

END DEFINE

