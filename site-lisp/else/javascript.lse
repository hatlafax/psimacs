DELETE LANGUAGE Javascript
DEFINE LANGUAGE Javascript
    /INITIAL_STRING="{javascript_program}"
    /PUNCTUATION_CHARACTERS="();.*,"
    /VALID_IDENTIFIER_CHARACTERS="a-zA-Z_0-9-"
    /INDENT_SIZE=2

END DEFINE

